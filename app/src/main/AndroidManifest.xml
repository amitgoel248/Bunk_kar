<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tutecentral.BUNK">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".splashscreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.tutecentral.BUNK.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".schedule"
            android:label="@string/title_activity_schedule" />
        <activity
            android:name=".subjects"
            android:label="@string/title_activity_subjects" />
        <activity
            android:name=".about"
            android:label="@string/title_activity_about" />
        <activity
            android:name=".calender"
            android:label="@string/title_activity_calender" />
        <activity
            android:name=".developers"
            android:label="@string/title_activity_developers" />
        <activity
            android:name=".main_components"
            android:label="@string/title_activity_main_components"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
